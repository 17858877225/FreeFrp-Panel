{
  "name": "freefrp-web",
  "version": "1.0.0",
  "description": "FreeFrp Web Management Platform",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write .",
    "update-version": "node bump-version.cjs"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx,vue,css,scss,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@vicons/ionicons5": "0.13.0",
    "axios": "^1.11.0",
    "dayjs": "^1.11.13",
    "naive-ui": "^2.42.0",
    "pinia": "3.0.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/node": "^20.19.9",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.33.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "vite": "7.0.6",
    "vue-tsc": "^1.8.27"
  }
}
